{
    "name": "studoc/qanda",
    "description": "Simple and humble CLI questions",
    "type": "project",
    "license": "MIT",
    "require": {
        "php": "^7.1.3",
        "fzaninotto/faker": "^1.9",
        "illuminate/database": "5.8.*",
        "laravel-zero/framework": "5.8.*",
        "nunomaduro/laravel-console-menu": "^2.1",
        "zendframework/zend-text": "^2.7"
    },
    "require-dev": {
        "mockery/mockery": "^1.0",
        "phpunit/phpunit": "^7.5"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-create-project-cmd": [
            "@php application app:rename"
        ]
    },
    "bin": ["qanda"]
}
